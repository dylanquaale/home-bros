nt\\src\\utils\\mutations.js",[],[],"C:\\Users\\Miguel\\OneDrive\\Desktop\\PROJECT3\\client\\src\\utils\\auth.js",["58"],[],"// use this to decode a token and get the user's information out of it\nimport decode from 'jwt-decode';\n\n// create a new class to instantiate for a user\nclass AuthService {\n  // get user data\n  getProfile() {\n    return decode(this.getToken());\n  }\n\n  // check if user's logged in\n  loggedIn() {\n    // Checks if there is a saved token and it's still valid\n    const token = this.getToken();\n    return !!token && !this.isTokenExpired(token); // handwaiving here\n  }\n\n  // check if token is expired\n  isTokenExpired(token) {\n    try {\n      const decoded = decode(token);\n      if (decoded.exp < Date.now() / 1000) {\n        return true;\n      } else return false;\n    } catch (err) {\n      return false;\n    }\n  }\n\n  getToken() {\n    // Retrieves the user token from localStorage\n    return localStorage.getItem('id_token');\n  }\n\n  login(idToken) {\n    // Saves user token to localStorage\n    localStorage.setItem('id_token', idToken);\n    window.location.assign('/');\n  }\n\n  logout() {\n    // Clear user token and profile data from localStorage\n    localStorage.removeItem('id_token');\n    // this will reload the page and reset the state of the application\n    window.location.assign('/');\n  }\n}\n\nexport default new AuthService();\n","C:\\Users\\Miguel\\OneDrive\\Desktop\\PROJECT3\\client\\src\\utils\\queries.js",[],[],"C:\\Users\\Miguel\\OneDrive\\Desktop\\PROJECT3\\client\\src\\utils\\localStorage.js",[],[],"C:\\Users\\Miguel\\OneDrive\\Desktop\\PROJECT3\\client\\src\\pages\\SearchProperties.js",["59","60"],[],"C:\\Users\\Miguel\\OneDrive\\Desktop\\PROJECT3\\client\\src\\pages\\SavedProperties.js",[],[],{"ruleId":"61","severity":1,"message":"62","line":49,"column":1,"nodeType":"63","endLine":49,"endColumn":34},{"ruleId":"64","severity":1,"message":"65","line":24,"column":25,"nodeType":"66","messageId":"67","end